msgid "Collection"
msgstr "Collectie"

#: adoration/forms.py:30
msgid "Select a period"
msgstr "Selecteer een periode"

#: adoration/forms.py:31 adoration/templates/adoration/registration.html:332
msgid "Period"
msgstr "Periode"

#: adoration/forms.py:36
msgid "Full Name *"
msgstr "Volledige naam *"

